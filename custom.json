{
"pageSetup" : {
  "size": "A4",
  "width": null,
  "height": null,
  "margin-top": "1cm",
  "margin-bottom": "1cm",
  "margin-left": "1cm",
  "margin-right": "1cm",
  "spacing-header": "5mm",
  "spacing-footer": "5mm",
  "margin-gutter": "0cm"
},

"pageTemplates" : {
  "coverPage": {
    "frames": [
      ["0cm", "0cm", "100%", "100%"]
    ],
    "showHeader" : false,
    "showFooter" : false
  },
  "oneColumn": {
    "frames": [
      ["0cm", "0cm", "100%", "100%"]
    ]
  },
  "twoColumn": {
    "frames": [
      ["0cm", "0cm", "38%", "100%"],
      ["40%", "0cm", "60%", "100%"]
    ]
  }
},

  "linkColor": "#3299bb",
  "styles": {
    "base": {
      "textColor": "#424242",
      "leading": 10
    },
    "title": {
      "alignment": "TA_LEFT",
      "fontSize": "140%"
    },
    "subtitle": {
      "fontSize": "90%"
    },
    "heading1": {
      "textColor": "#ff9900",
      "alignment": "TA_LEFT",
      "fontSize": "130%"
    },
    "heading2": {
      "fontSize": "110%",
      "spaceBefore": 18
    },
    "line": {
      "textColor": "#bcbcbc",
      "fontSize": 3,
      "leading": 0
    },
    "table-heading": {
      "backColor": "#eeeeee",
      "alignment": "TA_LEFT",
      "fontName": "stdBold"
    },
    "table": {
      "commands": [
        ["VALIGN", [0, 0], [-1, -1], "TOP"],
        ["INNERGRID", [0, 0], [-1, -1], 0.25, "#f7f7f7"],
        ["ROWBACKGROUNDS", [0, 0], [-1, -1], ["white"]],
        ["BOX", [0, 0], [-1, -1], 0.25, "#eeeeee" ]]
    }
  }
}
